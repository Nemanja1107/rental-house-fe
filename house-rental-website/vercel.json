{
    "version": 2,
    "builds": [
        {
            "src": "BE/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "FE/house-rental-website/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "BE/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "FE/house-rental-website/build/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}